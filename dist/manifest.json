{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "background": {
      "service_worker": "assets/background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["assets/content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "storage"
    ]
  }
  